    "enable-patching": true,
    "patches": {
      "drupal/classy_paragraphs": {
        "2830403 - Allow to filter classy styles": "https://www.drupal.org/files/issues/2020-08-28/choose_and_order-2830403-43_0.patch"
      },
      "drupal/core": {
        "Label Issue": "https://www.drupal.org/files/issues/2020-04-08/2985882-field-85.patch",
