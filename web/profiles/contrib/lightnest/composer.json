        "Youtube Privacy": "https://www.drupal.org/files/issues/2019-06-08/video_embed_field_youtube_nocookie_2973246_41.patch"
      },
      "drupal/seckit": {
        "3064669 - Remove div tag from header": "https://www.drupal.org/files/issues/2021-02-15/3064669-14.patch"
      },
      "drupal/lightning_media": {
        "3167843 - Unable to upload SVG": "https://www.drupal.org/files/issues/2022-07-01/unable-to-upload-svg-file-3167843-12.patch"
      },
      "drupal/lightning_core": {
        "3303770 - PHP 8.1: preg_replace(): Passing null to parameter #2 ($replacement) of type array|string is deprecated in UpdateManager.php line 129": "https://git.drupalcode.org/project/lightning_core/-/merge_requests/11.diff"
      },
      "drupal/metatag": {
        "2949963 - Merge special page metatags with node pages": "https://www.drupal.org/files/issues/2020-12-07/metatag-merge-node-special-pages-2949963-31.patch"
      },
      "drupal/recaptcha": {
        "2493183 - Ajax support / Use behaviors": "https://www.drupal.org/files/issues/2022-10-18/recaptcha-ajax-support-2493183-258.patch"
      }
    },
