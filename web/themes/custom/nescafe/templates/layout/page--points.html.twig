    // The following example creates complex markers to indicate beaches near
    // Sydney, NSW, Australia. Note that the anchor is set to (0,32) to correspond
    // to the base of the flagpole.
    function initMap() {
        const map = new google.maps.Map(document.getElementById("map"), {
            zoom: 8,
            center: { lat: 47.042349670286605, lng: 28.809768032644577 },
        });

        setMarkers(map);
    }

    // Data for the markers consisting of a name, a LatLng and a zIndex for the
    // order in which these markers should display on top of each other.
    const nescafe = [
        ["Orașul Edineț", 48.1700944009823, 27.304447698719915, 6, "Edinet", "<div class='map_text'><p><span><strong>Fourchette, str. Independenții, 90</b></span><br><span>Vineri 16.00 - 20.00</span><br><span>Sâmbătă 11.00 - 15.00</span></p></div>", "<a style='text-decoration: underline; color: #00a5e3' href='https://goo.gl/maps/urtqJorqYD6TyT6q9'>Google maps</a>"],
        ["Orașul Cahul", 45.9044151860974, 28.193958597178884, 5, "Cahul", "<div class='map_text'><p><span><strong>Linella, str. Independenței 3</b></span><br><span>Vineri 16.00 - 20.00</span><br><span>Sâmbătă 11.00 - 15.00</span></p></div>", "<a style='text-decoration: underline; color: #00a5e3' href='https://goo.gl/maps/KAd8CP59LoGEVTo98'>Google maps</a>"],
        ["Orașul Bălți", 47.78446094755549, 27.92502123931473, 4, "Bălți", "<div class='map_text'><p><span><strong>Linella, str. Kiev 6</b></span><br><span>Vineri 16.00 - 20.00</span><br></p><span>Sâmbătă 11.00 - 15.00</span></div>", "<a style='text-decoration: underline; color: #00a5e3' href='https://goo.gl/maps/YCfU9CY8qDExZYtG9'>Google maps</a>"],
        ["Orașul Comrat", 46.30707083089598, 28.65725019719525, 3, "Comrat", "<div class='map_text'><p><span><strong>Local, str. Lenin 146A</b></span><br><span>Vineri 16.00 - 20.00</span><br><span>Sâmbătă 11.00 - 15.00</span></p></div>", "<a style='text-decoration: underline; color: #00a5e3' href='https://goo.gl/maps/9M4kScXhGB4kp2EX7'>Google maps</a>"],
        ["Orașul Chișinău", 47.0137809290262, 28.856135968389154, 2, "Chișinău", "<div class='map_text'><p><span><strong>Bonus, str. Tiraspol 5</b></span><br><span>Vineri 16.00 - 20.00</span><br><span>Sâmbătă 11.00 - 15.00</span></p></div>", "<a style='text-decoration: underline; color: #00a5e3' href='https://goo.gl/maps/wSZve8KtmBsVW7657'>Google maps</a>"],
        ["Orașul Chișinău", 47.01104248206677, 28.84234003955409, 1, "NR1", "<div class='map_text'><p><span><strong>Nr.1, str. Lev Tolstoi 24/1</b></span><br><span>Vineri 16.00 - 20.00</span><br><span>Sâmbătă 11.00 - 15.00</span></p></div>", "<a style='text-decoration: underline; color: #00a5e3' href='https://goo.gl/maps/tVJ5dndicLP3zWjV8'>Google maps</a>"],
    ];

    function setMarkers(map) {
        const image = {
            url: "https://stag-yxxbuhy-qkl3gjov7gxou.eu-5.platformsh.site/sites/default/files/2023-06/pin.png",
            // This marker is 31 pixels wide by 37 pixels high.
            size: new google.maps.Size(31, 37),
            // The origin for this image is (0, 0).
            origin: new google.maps.Point(0, 0),
            // The anchor for this image is the base of the flagpole at (0, 37).
            anchor: new google.maps.Point(0, 37),
        };

        const shape = {
            coords: [1, 1, 1, 20, 18, 20, 18, 1],
            type: "poly",
        };

        for (let i = 0; i < nescafe.length; i++) {
            const nescafePoint = nescafe[i];

            const contentString =
                '<div id="content">' +
                '<div id="siteNotice">' +
                "</div>" +
                `<h1 id="firstHeading" class="firstHeading">`+ nescafePoint[4] +`</h1>` +
                '<div id="bodyContent">' +
                `<p>${nescafePoint[5]}</p>` +
                `<p>${nescafePoint[6]}</p>` +
            "</div>" +
            "</div>";

            const infowindow = new google.maps.InfoWindow({
                content: contentString,
                ariaLabel: "nescafe",
            });
            const marker = new google.maps.Marker({
                position: { lat: nescafePoint[1], lng: nescafePoint[2] },
                map,
                icon: image,
                shape: shape,
                title: nescafePoint[0],
                zIndex: nescafePoint[3],
            });

            marker.addListener("click", () => {
                infowindow.open({
                    anchor: marker,
                    map,
                });
            });
        }
    }

    window.initMap = initMap;
</script>
<script
  src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB83tmFmQPhdDZCLF-vHW4QCmwAPKcO66o&callback=initMap&v=weekly"
